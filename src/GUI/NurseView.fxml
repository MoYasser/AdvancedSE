<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="GUI.NurseView"
            prefHeight="400.0" prefWidth="600.0">
    <ImageView fx:id="img" fitHeight="500.0" fitWidth="600.0" layoutX="-3.0" pickOnBounds="true" preserveRatio="true">
        <Image url="/Resources/nurse.jpg"/>
    </ImageView>
    <Button layoutX="84.0" layoutY="154.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="127.0"
            text="View Salary"/>
    <Text fill="#c21c1c" layoutX="43.0" layoutY="89.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome Nurse">
        <font>
            <Font size="30.0"/>
        </font>
    </Text>
    <Button layoutX="84.0" layoutY="222.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="127.0"
            text="View Schedule"/>
    <Button fx:id="logOutBtn" layoutX="84.0" layoutY="292.0" mnemonicParsing="false" onAction="#logOut" prefHeight="50.0"
            prefWidth="127.0" text="Log out"/>
    <Text fill="#1d40c2" layoutX="487.0" layoutY="245.0" stroke="#f4f5f5" strokeType="OUTSIDE" strokeWidth="0.0"
          text="HCC" textAlignment="RIGHT">
        <font>
            <Font size="57.0"/>
        </font>
    </Text>

</AnchorPane>
